<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zzued.campustravel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.StartActivity"
            android:theme="@style/WindowFullScreen" />
        <activity
            android:name=".activity.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.HomePageActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.ThermalMapActivity" />
        <activity android:name=".activity.FlatMapActivity" />
        <activity android:name=".activity.RouteRecommendActivity" />
        <activity android:name=".activity.VoiceInteractActivity" />
        <activity android:name=".activity.VoiceAssistActivity" />
        <activity android:name=".activity.ScenicAreaIntroActivity" />
        <activity android:name=".activity.ScenicSpotIntroActivity"></activity>
    </application>

</manifest>